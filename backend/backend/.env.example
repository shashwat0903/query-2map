# Server Configuration
PORT=5000

# MongoDB Connection (update below with actual DB URI if self-hosted)
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<db_name>?retryWrites=true&w=majority

# Google OAuth Credentials (dummy or placeholder)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5000/auth/google/callback

# JWT Secret
JWT_SECRET=your_jwt_secret
